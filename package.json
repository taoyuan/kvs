{
  "name": "kvs",
  "version": "1.0.1",
  "description": "Simple key-value store facade for node.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "format": "prettier --write \"{src,test}/**/*.ts\" --single-quote --trailing-comma es5",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty",
    "test": "npm run build && nyc mocha --require ts-node/register --recursive \"test/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taoyuan/kvs.git"
  },
  "keywords": [
    "kv",
    "kvs",
    "key",
    "value",
    "key-value",
    "cache",
    "memory"
  ],
  "author": "Tao Yuan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taoyuan/kvs/issues"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "lodash": "~4.17.11",
    "lru-cache": "~5.1.1",
    "minimatch": "^3.0.4"
  },
  "devDependencies": {
    "@types/async": "^2.4.0",
    "@types/bluebird": "^3.5.25",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/lru-cache": "^4.1.1",
    "@types/minimatch": "^3.0.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.21",
    "@types/sinon": "^7.0.5",
    "chai": "~4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "~5.2.0",
    "nyc": "^13.2.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sinon": "~7.2.3",
    "ts-node": "8.0.2",
    "typescript": "3.3.1"
  }
}
