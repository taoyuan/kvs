{
  "name": "kvs-testlab",
  "description": "A common test suite for KVS APIs",
  "version": "0.1.4",
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "author": "ty ",
  "license": "MIT ",
  "scripts": {
    "acceptance": "tib-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "tib-tsc",
    "clean": "tib-clean kvs-testlab*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "tib-mocha \"dist/__tests__/**/*.js\"",
    "unit": "tib-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf kvs-testlab*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@tib/build": "^6.3.4",
    "@tib/eslint-config": "^8.2.4",
    "@tib/testlab": "^3.4.4",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "cross-env": "^7.0.2",
    "mocha": "^8.1.3",
    "np": "^6.5.0"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ]
}
